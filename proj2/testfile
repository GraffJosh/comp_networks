#!/usr/bin/expect
spawn ./project2
expect ":"
send "10000\n"		
			#numpkt
expect ":"
send "0.10\n"	
			#loss
expect ":"
send "0.5\n"
			#corrupt
expect ":"
send ".5\n"
			#order
expect ":"
send "20\n"
			#time
expect ":"
send "0\n"
			#trace
expect ":"
send "1\n"
			#randomized
expect ":"
send "0\n"
			#bidirectional
# turn it back over to interactive user
interact
