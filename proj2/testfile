#!/usr/bin/expect
spawn ./project2
expect ":"
send "10000\n"		
			#numpkt
expect ":"
send "0.1\n"	
			#loss
expect ":"
send "0.5\n"
			#corrupt
expect ":"
send ".5\n"
			#order
expect ":"
send "5\n"
			#time
expect ":"
send "0\n"
			#trace
expect ":"
send "0\n"
			#randomized
expect ":"
send "0\n"
			#bidirectional
# turn it back over to interactive user
interact